{
  "name": "haydarsahin/cache-migration",
  "description": "The package helpful about migrate redis cache for seamless deploys",
  "license": "MIT",
  "keywords": [
    "laravel",
    "redis",
    "database",
    "migration",
    "cache-migration"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Haydar ŞAHİN",
      "email": "mail@haydarsahin.com",
      "homepage": "http://haydarsahin.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/support": "^5.5|^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^3.5|^3.6|^3.7|^3.8|^4.0|^5.0|^6.0",
    "phpunit/phpunit": "~6.0|^7.0|^7.5|^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "HaydarSahin\\CacheMigration\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HaydarSahin\\CacheMigration\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "HaydarSahin\\CacheMigration\\CacheMigrationServiceProvider"
      ]
    }
  }
}
